pab_camp.gat,30,205,4	script	補給兵長#pab_camp	415,{/* 51130 */
	while(1) {
		mes "[補給兵長]";
		mes "「亡国の硬貨」をお持ちであれば";
		mes "そちらの補給兵のものとは異なる";
		mes "特別なアイテムを提供いたします。";
		mes "　";
		mes "^0000ff‐「亡国の硬貨」所持数：0‐^000000";
		next;
		switch(select("アイテムと交換する","交換アイテムを確認したい","話をやめる")) {
		case 1:
			mes "[補給兵長]";
			mes "かしこまりました。";
			mes "用意しているものの中から";
			mes "お選びください。";
			mes "なお「エルヴィラキャンディ」は";
			mes "1日1回だけ交換していただけます。";
			next;
			switch(select("呪詛の魔石","エルヴィラキャンディ","古いカード帖の中身","ホムンクルスとの親密度アップ","やっぱりやめる")) {
			case 1:
				break;
			case 2:
				mes "[補給兵長]";
				mes "亡国の硬貨50枚で";
				mes "15個お渡しいたします。";
				mes "再交換は、次の午前5時以降まで";
				mes "待っていただくことになります。";
				mes "　";
				mes "^0000ff‐「亡国の硬貨」所持数：0‐^000000";
				next;
				if(select("エルヴィラキャンディをもらう","やっぱりやめる") == 2) {
					mes "[補給兵長]";
					mes "かしこまりました。";
					mes "御入用のものがありましたら";
					mes "またお越しください。";
					close;
				}
				mes "[補給兵長]";
				mes "申し訳ございませんが";
				mes "亡国の硬貨が足りないようです。";
				mes "必要な数を集めてから";
				mes "またお越しください。";
				close;
			case 3:
				mes "[補給兵長]";
				mes "「^ff0000古いカード帖^000000」を開封して";
				mes "中身をお渡しします。";
				mes "亡国の硬貨2500枚で1回開封します。";
				mes "　";
				mes "^0000ff‐「亡国の硬貨」所持数：0‐^000000";
				next;
				mes "[補給兵長]";
				mes "申し訳ございませんが";
				mes "亡国の硬貨が足りないようです。";
				mes "必要な数を集めてから";
				mes "またお越しください。";
				close;
			case 4:
				mes "[補給兵長]";
				mes "ホムンクルスとの親密度を上げる";
				mes "不思議な薬を用意しました。";
				mes "こちらを使わせていただきます。";
				next;
				mes "[補給兵長]";
				mes "亡国の硬貨1500枚で";
				mes "1回ご使用いただけます。";
				mes "　";
				mes "^0000ff‐「亡国の硬貨」所持数：0‐^000000";
				next;
				mes "[補給兵長]";
				mes "申し訳ありませんが";
				mes "冒険者様のホムンクルスが";
				mes "見当たりません。";
				mes "ホムンクルスを呼び出した状態で";
				mes "お声掛けください。";
				close;
			case 5:
				mes "[補給兵長]";
				mes "かしこまりました。";
				mes "御入用のものがありましたら";
				mes "またお越しください。";
				close;
			}
			while(1) {
				mes "[補給兵長]";
				mes "「^ff0000呪詛の魔石^000000」は";
				mes "1個^ff0000300^000000枚と交換です。";
				mes "何個交換しますか？";
				mes "一度に1000個まで交換可能です。";
				mes "　";
				mes "^0000ff‐「亡国の硬貨」所持数：0‐^000000";
				next;
				input '@num;
				if('@num == 0) {
					mes "[補給兵長]";
					mes "0セットですので";
					mes "交換を取りやめます。";
					mes "また交換したい時は";
					mes "改めてお声掛けください。";
					close;
				}
				if('@num < 0 || '@num > 1000) {
					mes "[補給兵長]";
					mes "申し訳ありませんが";
					mes "1セットから1000セットの間で";
					mes "指定していただけますか？";
					next;
					continue;
				}
				break;
			}
			mes "[補給兵長]";
			mes "「^ff0000呪詛の魔石^000000（1個）」が";
			mes "「^ff00001^000000」セットですので";
			mes "「^ff0000300^000000」枚です。";
			mes "　";
			mes "^0000ff‐「亡国の硬貨」所持数：0‐^000000";
			next;
			if(select("交換する","やっぱりやめる") == 2) {
				mes "[補給兵長]";
				mes "承知しました。";
				mes "また何かございましたら";
				mes "お声掛けください。";
				close;
			}
			mes "[補給兵長]";
			mes "申し訳ございませんが";
			mes "亡国の硬貨が足りないようです。";
			mes "必要な枚数を集めてから";
			mes "またお越しください。";
			close;
		case 2:
			mes "[補給兵長]";
			mes "こちらが提供している";
			mes "アイテムのリストです。";
			next;
			mes "●------補給物資リスト------●";
			mes "■呪詛の魔石：1個";
			mes "　必要枚数：【^ff0000300^000000】";
			mes "■エルヴィラキャンディ：15個";
			mes "　必要枚数：【^ff000050^000000】";
			mes "■古いカード帖の中身：1個";
			mes "　必要枚数：【^ff00002500^000000】";
			mes "■ホムンクルスとの親密度アップ：1回";
			mes "　必要枚数：【^ff00001500^000000】";
			next;
			continue;
		case 3:
			mes "[補給兵長]";
			mes "かしこまりました。";
			mes "御入用のものがありましたら";
			mes "またお越しください。";
			close;
		}
	}
}
