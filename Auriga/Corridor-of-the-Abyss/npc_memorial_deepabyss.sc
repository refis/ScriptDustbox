pab_camp.gat,25,181,5	script	衛生兵#pab_camp	95,{/* 51124 */
	mes "[衛生兵]";
	mes "お待ちしておりました、冒険者様。";
	mes "傷の治療や位置セーブ、各種レンタルの";
	mes "サービスを行っております。";
	mes "ぜひご利用ください。";
	next;
	switch(select("傷の治療","位置セーブ","各種レンタル","やめる")) {
	case 1:
		mes "[衛生兵]";
		mes "傷の治療はHPとSPを";
		mes "^0000ff5000 Zeny^000000で100%まで";
		mes "回復するサービスです。";
		mes "よろしいですか？";
		next;
		if(select("はい","いいえ") == 2) {
			mes "[衛生兵]";
			mes "かしこまりました。";
			mes "必要になりましたら";
			mes "いつでもお越しください。";
			mes "お待ちしております。";
			close;
		}
		//fail
		close;
	case 2:
		mes "[衛生兵]";
		mes "位置セーブは戦闘不能時に";
		mes "帰還する場所を現在地周辺に";
		mes "更新するサービスです。";
		mes "よろしいですか？";
		next;
		if(select("はい","いいえ") == 2) {
			mes "[衛生兵]";
			mes "かしこまりました。";
			mes "必要になりましたら";
			mes "いつでもお越しください。";
			mes "お待ちしております。";
			close;
		}
		savepoint "pab_camp.gat",27,177;
		mes "[衛生兵]";
		mes "それでは記録いたします。";
		mes "ご利用ありがとうございます。";
		close;
	case 3:
		mes "[衛生兵]";
		mes "各種レンタルはカートなどの";
		mes "貸し出しを行うサービスです。";
		mes "どのレンタルをご希望ですか？";
		next;
		mes "[衛生兵]";
		mes "あっ……。";
		mes "お客様の職業は";
		mes "貸し出せるものがないようです。";
		mes "申し訳ございません…。";
		mes "他のサービスをご利用ください。";
		close;
	case 4:
		mes "[衛生兵]";
		mes "かしこまりました。";
		mes "必要になりましたら";
		mes "いつでもお越しください。";
		mes "お待ちしております。";
		close;
	}
}
