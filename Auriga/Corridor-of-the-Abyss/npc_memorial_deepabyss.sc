pab_camp.gat,15,174,5	script	運び屋#pab_camp	794,{/* 51118 */
	mes "[運び屋]";
	mes "……";
	mes "お前は冒険者だな？";
	mes "どこから嗅ぎつけたか知らんが";
	mes "俺に声をかけたってことは";
	mes "そういうことだろう？";
	next;
	switch(select("いいえ","そうだ","……？")) {
	case 1:
		mes "[運び屋]";
		mes "冷やかしか……";
		mes "お前と話すことはない。";
		close;
	case 2:
		mes "[運び屋]";
		mes "いいだろう。";
		mes "10,000 zenyで、お前を運んでやる。";
		mes "前払いだ。";
		next;
		if(select("いいえ","はい") == 1) {
			mes "[運び屋]";
			mes "冷やかしか……";
			mes "金を払えない奴と話すことはない。";
			close;
		}
		if(Zeny < 10000) {
			mes "[運び屋]";
			mes "これっぽちしか";
			mes "持っていないのか？";
			mes "金を払えない奴と話すことはない。";
			close;
		}
		mes "[運び屋]";
		mes "少し揺れるぞ。";
		mes "しっかり掴まってろ。";
		mes "振り落とされても知らんからな。";
		close2;
		set Zeny,Zeny-10000;
		warp "pab_base01",338,75;
		end;
	case 3:
		mes "[運び屋]";
		mes "…………。";
		next;
		mes "[運び屋]";
		mes "……";
		mes "最近、洞窟を抜けた先にある";
		mes "城に行きたいという";
		mes "冒険者が多くてな。";
		next;
		mes "[運び屋]";
		mes "あんな場所に、何の用があるのか";
		mes "知らんし、興味も無いが";
		mes "金になるから運んでやってるんだ。";
		next;
		mes "[運び屋]";
		mes "あまり口外するなよ。";
		mes "行ったきり帰ってこない奴もいる。";
		mes "俺は運ぶだけで、それ以上のことに";
		mes "首を突っ込むつもりはないんでな。";
		close;
	}
}
pab_camp.gat,19,205,5	script	メロン鑑定士#pab_camp	91,{/* 51119 */
	mes "[メロン鑑定士]";
	mes "私は、一級メロン鑑定士です。";
	mes "メロンのことなら、お任せください。";
	mes "もし^ff0000未鑑定メロン^000000をお持ちでしたら";
	mes "私が鑑定して差し上げますよ。";
	set '@str$,((countitem(25741)<1)? "^888888": "^000000") + "鑑定する^000000";
	next;
	switch(select('@str$,"メロン鑑定士？","やめる")) {
	case 1:
		break;
	case 2:
		mes "[メロン鑑定士]";
		mes "ヒヨコ鑑定士って、ありますよね？";
		mes "ヒヨコのオスとメスを仕分ける職業です。";
		next;
		mes "[メロン鑑定士]";
		mes "それと同じものと思ってくれれば";
		mes "おおよそ間違いありませんわ。";
		close;
	case 3:
		mes "[メロン鑑定士]";
		mes "それは残念です。";
		mes "御用の際は、お声かけくださいね。";
		close;
	}
	if(countitem(25741) < 1) {
		mes "[メロン鑑定士]";
		mes "「未鑑定メロン」を";
		mes "お持ちでないようです。";
		mes "王城のある山の麓に生息している";
		mes "「メロリン」を駆除すれば";
		mes "手に入ることがありますよ。";
		mes "手に入れたら、来てくださいね。";
		close;
	}
	//TODO
	set '@max,countitem(25741);
	if(MaxWeight - Weight < '@max * 1500) {
		set '@max,(MaxWeight - Weight) / 1500;
	}
	if(checkitemblank() < '@max) {
		set '@max,checkitemblank();
	}
	mes "[メロン鑑定士]";
	mes "いくつ鑑定いたしましょうか？";
	mes "冒険者様の空き重量ですと";
	mes "「" +'@max+ "」個まで";
	mes "鑑定が可能です。";
	mes "「0」個でしたら取りやめます。";
	next;
	switch(select("所持限界まで","個数を指定する","やめる")) {
	case 1:
		set '@num,'@max;
		break;
	case 2:
		input '@num;
		if('@num == 0) {
			mes "[メロン鑑定士]";
			mes "かしこまりました。";
			mes "御用の際は、お声かけくださいね。";
			close;
		}
		break;
	default:
		mes "[メロン鑑定士]";
		mes "かしこまりました。";
		mes "御用の際は、お声かけくださいね。";
		close;
	}
	mes "[メロン鑑定士]";
	mes "それでは早速……";
	mes "……ペロ。";
	misceffect 441, "メロン鑑定士#pab_camp"; //1135
	mes "……この味は……!!!!";
	mes "終わりました。";
	next;
	for(set '@i,0; '@i < '@num; set '@i,'@i+1) {
		if(countitem(25741) == 0)
			break;
		delitem 25741,1;
		getitem 25742,1;
	}
	mes "[メロン鑑定士]";
	mes "素晴らしいメロンを";
	mes "鑑定する機会に巡り合えたこと";
	mes "神様に感謝いたします！";
	mes "またの機会を、お待ちしておりますわ。";
	close;
}
