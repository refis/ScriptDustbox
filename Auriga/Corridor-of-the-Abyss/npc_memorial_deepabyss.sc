	mes "　みすぼらしい門がある。";
	mes "　門の隙間からは、この世の全てを";
	mes "　憎悪するかのような";
	mes "　おぞましい怨念がにじみ出ている‐^000000";
	if(ismemorial())
		set '@str$,"^ee82ee深淵の王墓^000000へ入場";
	else if(getpartyleader(getcharid(1)) == strcharinfo(0))
		set '@str$,"^ee82ee深淵の王墓(Royal tombs of abyss)^000000の作成";
	next;
	switch(select("立ち去る",'@str$,"^ee82ee深淵の王墓^000000の注意事項を聞く")) {
	case 1:
		mes "^0000ff‐門の前から立ち去った‐^000000";
		close;
	case 2:
		if(ismemorial()) {
			mes "[シキナ]";
			mes "深淵の王墓に関する注意事項を";
			mes "確認したい時は";
			mes "先に聞いて欲しい。";
			mes "　";
			mes "王墓へ向かうのだな？";
			next;
			if(select("少し考える","^ff0000王墓へ向かう^000000") == 1) {
				mes "[シキナ]";
				mes "了解した。";
				mes "十分に準備を整えてから";
				mes "挑戦してくれ。";
				close;
			}
			mes "[シキナ]";
			mes "私も共に行こう。";
			mes "よろしく頼むぞ！";
			close2;
			announce "メモリアルダンジョン[Royal tombs of abyss] に入場しました　：　" +strcharinfo(1)+ " (" +strcharinfo(0)+ ")", 0x9, 0x00ff99, 0x190, 12, 0, 0;
			//warp "1@gt_00.gat",226,38;
			end;
		}
		if(checkquest(123620)) {
			mes "[シキナ]";
			mes "前回、怨嗟の門を開いた際に";
			mes "そなたの身が呪われてから";
			mes "あまり時間が経ってないようだ。";
			mes "呪いの力が浄化されるまで";
			mes "待ってもらう必要がある。";
			next;
			mes "^ff0000[インフォメーション]";
			mes "深淵の王墓は";
			mes "「深淵の王墓　生成時間制限」に";
			mes "記載されている日時以降に";
			mes "再生成が可能です。";
			mes "詳細はクエストリスト(Alt + U)を";
			mes "ご覧ください。^000000";
			close;
		}
		if(!checkquest(123707)) {
			misceffect 30, "EF_01#pab_camp"; //50954
			misceffect 622, "EF_01#pab_camp"; //50954
			misceffect 30, "EF_02#pab_camp"; //50955
			misceffect 622, "EF_02#pab_camp"; //50955
			misceffect 30, "EF_03#pab_camp"; //50956
			misceffect 622, "EF_03#pab_camp"; //50956
			misceffect 30, "EF_04#pab_camp"; //50957
			misceffect 622, "EF_04#pab_camp"; //50957
			misceffect 780, "怨嗟の門#pab_camp"; //50953
			misceffect 812, "怨嗟の門#pab_camp"; //50953
			misceffect 800, "怨嗟の門#pab_camp"; //50953
			misceffect 487, "怨嗟の門#pab_camp"; //50953
			misceffect 589, "怨嗟の門#pab_camp"; //50953
			misceffect 826, "怨嗟の門#pab_camp"; //50953
			mes "[シキナ]";
			mes "その場所こそ呪詛の王が";
			mes "埋葬された場所であり";
			mes "封印されている場所へ繋がる門だ。";
			next;
			mes "[シキナ]";
			mes "不用意に近づくのはやめておけ。";
			mes "耐性のない者が近付けば";
			mes "正気を失う恐れがあるぞ。";
			mes "もっとも……鍵を持たぬ者が";
			mes "門を開けることは不可能だがな。";
			close;
		}
		mes "[シキナ]";
		mes "一度、深淵の王墓の作成を開始すると";
		mes "次の午前5時までは作成が行えないが、";
		mes "問題はないか？";
		next;
		mes "^ff0000[インフォメーション]";
		mes "現在加入しているパーティーに";
		mes "作成中または作成済みの";
		mes "メモリアルダンジョンが存在する場合、";
		mes "ダンジョンの作成に";
		mes "失敗する場合があります。^000000";
		next;
		mes "^ff0000[インフォメーション]";
		mes "作成に失敗した場合でも";
		mes "「深淵の王墓　生成時間制限」が";
		mes "付与され、記載されている";
		mes "日時以降まで再生成を";
		mes "行うことができませんので";
		mes "ご注意ください。^000000";
		next;
		if(select("少し考える","^ff0000問題ない^000000") == 1) {
			mes "[シキナ]";
			mes "了解した。";
			mes "十分に準備を整えてから";
			mes "挑戦してくれ。";
			close;
		}
		if(getpartyleader(getcharid(1)) != strcharinfo(0))
			close;
		setquest 123620; //state=1
		misceffect 30, "EF_01#pab_camp"; //955
		misceffect 622, "EF_01#pab_camp"; //955
		misceffect 30, "EF_02#pab_camp"; //956
		misceffect 622, "EF_02#pab_camp"; //956
		misceffect 30, "EF_03#pab_camp"; //957
		misceffect 622, "EF_03#pab_camp"; //957
		misceffect 30, "EF_04#pab_camp"; //958
		misceffect 622, "EF_04#pab_camp"; //958
		misceffect 780, "怨嗟の門#pab_camp"; //954
		misceffect 812, "怨嗟の門#pab_camp"; //954
		misceffect 800, "怨嗟の門#pab_camp"; //954
		mes "■パーティー名";
		mes "^009eff" +getpartyname(getcharid(1))+ "^000000";
		mes "■パーティーリーダー";
		mes "^009eff" +getpartyleader(getcharid(1))+ "^000000";
		mes "　";
		mes "^006400Royal tombs of abyss^000000の作成申請を受け付けます。";
		next;
		mes "[シキナ]";
		mes "^ee82ee深淵の王墓(Royal tombs of abyss)^000000の";
		mes "作成が完了したら";
		mes "もう一度、^ee82ee怨嗟の門^000000を";
		mes "調べてくれ。";
		close;
	case 3:
		mes "[シキナ]";
		mes "「深淵の王墓」について説明しよう。";
		mes "まずは、「怨嗟の門」を開くと";
		mes "その際、そなたの身に呪いがかかり";
		mes "^ff0000午前5時以降にならないと";
		mes "新しく深淵の王墓への門を";
		mes "開くことができなくなる^000000。";
		next;
		mes "^0000ff【インフォメーション】";
		mes "‐現在組んでいるパーティーが";
		mes "　他のメモリアルダンジョンを";
		mes "　作成している等";
		mes "　エントリーに失敗しても";
		mes "　鍵は消滅してしまいます‐^000000";
		next;
		mes "[シキナ]";
		mes "鍵を使い、中に入ったら";
		mes "私が案内するので";
		mes "私の後について来てくれ。";
		next;
		mes "[シキナ]";
		mes "最初の通路から次のフロアに進んだ際";
		mes "そなたには120分間と";
		mes "午前5時まで続く";
		mes "2つの呪いが付与される。";
		next;
		mes "[シキナ]";
		mes "この呪いが付与された後は";
		mes "^FF0000呪いを掛けられた王墓への";
		mes "再入場は何度でも可能^000000だ。";
		next;
		mes "[シキナ]";
		mes "ただし呪いがとけるまでは";
		mes "怨嗟の門を新たに開いたり";
		mes "他の冒険者が開いた別の王墓に";
		mes "入っても先に進むことはできない。";
		mes "重要なことだから覚えておいてくれ。";
		next;
		mes "[シキナ]";
		mes "さて……王墓の中では";
		mes "死闘を繰り返すことになる。";
		mes "恐らく力尽きてしまうことも";
		mes "あるだろう。";
		next;
		mes "[シキナ]";
		mes "だが、「^ff00003^000000」回までであれば";
		mes "私の術を使って";
		mes "進行中のフロアから";
		mes "^ff0000再挑戦することが可能^000000だ。";
		next;
		mes "[シキナ]";
		mes "戦っていた最中の敵に関しては";
		mes "最初の状態からやり直しとなる。";
		mes "また「3」回以上のやり直しは";
		mes "不可能だから気を付けてくれ。";
		next;
		mes "[シキナ]";
		mes "説明は以上だ。";
		mes "覚悟して、深淵の王墓に";
		mes "挑んで欲しい。";
		close;
	}
OnInit:
	initnpctimer;
	end;
OnTimer5000:
	misceffect 487, "怨嗟の門#pab_camp"; //50953
	misceffect 589, "怨嗟の門#pab_camp"; //50953
	misceffect 826, "怨嗟の門#pab_camp"; //50953
	initnpctimer;
	end;
}
