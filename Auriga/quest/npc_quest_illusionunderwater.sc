		mes "　";
		mes "^0000ff下層の深海生物の処理^000000";
		//mes "^0000ff下層の深海の魚人族の処理^000000";
		mes "^0000ff討伐受注中^000000";
		next;
		switch(select("今のまま討伐を続ける","討伐対象の変更","討伐依頼のキャンセル")) {
		case 1:
			mes "‐討伐を続けることにした‐";
			close;
		case 2:
			mes "^ff0000【インフォメーション】";
			mes "討伐対象を変更すると";
			mes "変更前の討伐は破棄されます。^000000";
			mes "　";
			mes "^0000ff下層の深海生物の処理^000000";
			mes "^0000ff討伐受注中^000000";
			next;
			switch(select("今のまま討伐を続ける","対象を^0000ff上層の深海生物の処理^000000に変更する","対象を^0000ff下層の深海の魚人族の処理^000000に変更する")) {
			case 2:
				delquest 129365;
				delquest 3417;
				setquest 3415;	// state=1
